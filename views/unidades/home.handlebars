{{> navbar}}
<div class="container page-margin-top">
  <div class="row">
    <div class="col-2">{{> navUnidades}}</div>
    <div class="col-10">
      <h1>Cadastro de unidades</h1>
      <hr>
      <form action="/unidades/und/insertund" method="post">

        <div class="form-group">
          <label for="cnpj">CNPJ</label>
          <input type="text" name="cnpj" class="form-control" placeholder="CNPJ da unidade">
        </div>

        <div class="form-group">
          <label for="nome">Nome:</label>
          <input type="text" name="nome" class="form-control" placeholder="Nome da unidade">
        </div>

        <div class="form-group">
          <label for="endereco">Endereço:</label>
          <input type="text" name="endereco" class="form-control" placeholder="Endereço da unidade">
        </div>

        <div class="form-group">
          <label for="telefone">Telefone:</label>
          <input type="text" name="telefone" class="form-control" placeholder="Telefone da unidade">
        </div>

        <input type="submit" value="Cadastrar" class="btn btn-primary px-3 m-3">
      </form>
    </div>
  </div>
</div>